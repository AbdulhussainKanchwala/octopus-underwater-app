{
    "family": "emsbilling-task-defention", 
    "networkMode": "awsvpc", 
    "executionRoleArn": "arn:aws:iam::027522505557:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::027522505557:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "emsbilling-image", 
            "image": "027522505557.dkr.ecr.us-east-1.amazonaws.com/emswebuiapplication:latest", 
            "portMappings": [
                {
                    "containerPort": 8000, 
                    "hostPort": 8000, 
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/emsbilling-task-defention",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "ecs"
            }
         }
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "1024", 
    "memory": "2048"
}

