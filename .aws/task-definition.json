{
    "family": "testapp-task", 
    "networkMode": "awsvpc", 
    "executionRoleArn": "arn:aws:iam::027522505557:role/myECcsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::027522505557:role/myECcsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "testapp", 
            "image": "027522505557.dkr.ecr.us-west-1.amazonaws.com/emsimages:latest", 
            "portMappings": [
                {
                    "containerPort": 8000, 
                    "hostPort": 8000, 
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/emsbilling-task-defention",
              "awslogs-region": "us-west-1",
              "awslogs-stream-prefix": "ecs"
            }
         }
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "1024", 
    "memory": "2048"
}

